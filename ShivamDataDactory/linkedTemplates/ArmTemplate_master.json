		"filesSQLDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "olistproject_diagramdot"
		},
		"filesSQLDB_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "olistproject_diagramdot"
		},
